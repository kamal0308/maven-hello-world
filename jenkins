pipeline{
    agent any
    stages{
        stage("GIT_CHECKOUT"){
            steps{
               git 'https://github.com/kamal0308/maven-hello-world.git'
            }
        }
        stage("Maven clean"){
            steps{
             withMaven(maven: 'maven') {
                sh "mvn clean compile"
            }
        }
    }
}
}
