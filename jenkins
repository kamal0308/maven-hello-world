pipeline{
    agent any
    stages{
        stage("GIT_CHECKOUT"){
            steps{
               git 'https://github.com/kamal0308/maven-hello-world.git'
            }
        }
        stage("Maven build"){
            steps{
            with Maven(maven: 'maven') {
                sh "mvn compiler:compile"
            }
        }
    }
}
}
